\\u0442\\u0430\\u0432\\u0438\\u0442\\u044C \\u0434\\u043E\\u043F\\u043E\\u043B\\u043D\\u0438\\u0442\\u0435\\u043B\\u044C\\u043D\\u0443\\u044E \\u0438\\u043D\\u0444\\u043E\\u0440\\u043C\\u0430\\u0446\\u0438\\u044E, \\u0447\\u0442\\u043E\\u0431\\u044B \\u043F\\u043E\\u043C\\u043E\\u0447\\u044C \\u0441\\u0443\\u0434\\u044C\\u044F\\u043C \\u0432 \\u043F\\u043E\\u043D\\u0438\\u043C\\u0430\\u043D\\u0438\\u0438 \\u0438\\u0445 \\u0440\\u0430\\u0431\\u043E\\u0442\\u044B. \\u042D\\u0442\\u043E \\u043C\\u0435\\u0441\\u0442\\u043E, \\u0433\\u0434\\u0435 \\u043A\\u043E\\u043C\\u0430\\u043D\\u0434\\u044B \\u043C\\u043E\\u0433\\u0443\\u0442 \\u043F\\u0440\\u0435\\u0434\\u043E\\u0441\\u0442\\u0430\\u0432\\u0438\\u0442\\u044C \\u0431\\u043E\\u043B\\u0435\\u0435 \\u043F\\u043E\\u0434\\u0440\\u043E\\u0431\\u043D\\u0443\\u044E \\u0438\\u043D\\u0444\\u043E\\u0440\\u043C\\u0430\\u0446\\u0438\\u044E \\u043E \\u0441\\u0432\\u043E\\u0438\\u0445 \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u0430\\u0445. \\u041E\\u043D\\u0430 \\u0434\\u043E\\u043B\\u0436\\u043D\\u0430 \\u0431\\u044B\\u0442\\u044C \\u043E\\u0440\\u0433\\u0430\\u043D\\u0438\\u0437\\u043E\\u0432\\u0430\\u043D\\u0430 \\u0442\\u0430\\u043A\\u0438\\u043C \\u043E\\u0431\\u0440\\u0430\\u0437\\u043E\\u043C, \\u0447\\u0442\\u043E\\u0431\\u044B \\u043E\\u0442\\u0434\\u0435\\u043B\\u044C\\u043D\\u044B\\u0435 \\u0442\\u0435\\u043C\\u044B \\u0431\\u044B\\u043B\\u0438 \\u043B\\u0435\\u0433\\u043A\\u043E \\u0438\\u0434\\u0435\\u043D\\u0442\\u0438\\u0444\\u0438\\u0446\\u0438\\u0440\\u043E\\u0432\\u0430\\u043D\\u044B \\u0438 \\u0438\\u0445 \\u043C\\u043E\\u0436\\u043D\\u043E \\u0431\\u044B\\u043B\\u043E \\u0431\\u044B\\u0441\\u0442\\u0440\\u043E \\u043D\\u0430\\u0439\\u0442\\u0438.\"));\n}\n\nexport default DocNineComponent;","map":{"version":3,"sources":["/home/aruzhan/Desktop/project/aenta/src/routes/docNine/DocComponentNine.js"],"names":["React","DocNineComponent"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,kCAAP;AACA,OAAO,aAAP;;AAKA,SAASC,gBAAT,GAA4B;AACxB,sBACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kMADF,eAEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,26GAFF,eAIR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6FAJQ,eAMR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ijDANQ,eAOR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2HAPQ,eAQR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wiBARQ,eASR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+GATQ,eAUR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,26CAVQ,eAWR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kLAXQ,eAYR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,goEAZQ,eAaR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6KAbQ,eAcR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ixCAdQ,eAeR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,i0DAfQ,eAiBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oJAjBQ,eAkBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ksEAlBQ,eAmBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kvCAnBQ,eAoBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0OApBQ,eAsBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,m/DAtBQ,CADJ;AA0BH;;AAED,eAAeA,gBAAf","sourcesContent":["import React from 'react';\nimport 'bootstrap/dist/css/bootstrap.css';\nimport './index.css'\n\n\n\n\nfunction DocNineComponent() {\n    return (\n        <div>\n          <h3>Форма подачи заявки на получение Control Award</h3>\n          <p>Чтобы претендовать на Control Award, команды должны представить форму подачи заявки на Control Award. В этой форме команды определяют и обобщают ключевые элементы управления, которые делают их робота уникальным. Включено описание ключевых наблюдаемых действий, на которые следует обратить внимание судьям, а также используемых датчиков и алгоритмов, которые делают все это возможным. Судьи будут использовать эту форму как для оценки контрольных конструкций, так и при наблюдении за роботами на поле соревнований. Команды должны определить аспекты управления своим роботом, которыми они больше всего гордятся. Форма подачи заявки на получение контрольной премии не может превышать 2 страниц.\n</p>\n<p>Автономные цели\n</p>\n<p>Перечислите общие действия, которые может выполнить робот. Они должны включать в себя подсчитывающие счет действия, а также другие позиционные и оборонительные операции. Робот не обязан выполнять все это в каждой программе, но должен быть продемонстрирован по крайней мере в одной автономной программе.</p>\n<p>Используемые датчики</p>\n<p>Перечислите датчики, используемые для управления роботом, и краткое описание того, как они используются.</p>\n<p>Ключевые алгоритмы</p>\n<p>Перечислите ключевые алгоритмы, которые делают вашего робота уникальным или жизненно важны для его успеха на поле боя. Особенно сложные или уникальные алгоритмы или те, которые объединяют использование нескольких датчиков, являются хорошими кандидатами для выделения здесь.</p>\n<p>Улучшения управления драйвером</p>\n<p>Перечислите все дополнительные элементы управления, которые используются в течение периода, контролируемого драйвером, для повышения производительности. Они могут включать операции сигнализации при обнаружении определенного состояния на поле, функции автозаполнения, безотказные алгоритмы или просто любые усовершенствования, которые облегчают или повышают эффективность управления роботом для драйвера.</p>\n<p>Ссылки на инженерное портфолио</p>\n<p>Судьи также используют инженерное портфолио команд для оценки деталей элементов управления. Чтобы помочь направить эти усилия, команды должны предоставить указания на то, где в инженерном портфолио находится информация, связанная с управлением.</p>\n<p>Некоторые моменты, которые следует указывать в качестве указателей: Цели команды для действий по управлению, стратегии для автономного режима, производительность робота с добавленными датчиками и без них, требования для успешной автономной работы, улучшения производительности с использованием алгоритмов и датчиков, а также результаты тестирования.\n</p>\n<p>Схемы автономных программ</p>\n<p>Для автономных операций команды должны нарисовать и обозначить типичный путь, по которому движется робот. Помеченные точки определяют ключевые наблюдаемые действия, которые совершает робот. Для каждой отмеченной точки следует отметить краткое описание того, что происходит (см. пример ниже). Особенно опишите те ключевые операции, в которых вносятся корректировки для обеспечения точной и воспроизводимой производительности.</p>\n<p>Для команд с несколькими автономными программами нет необходимости документировать каждую программу на отдельном листе. Достаточно задокументировать наиболее часто используемые или сложные программы и отметить различия для остальных.</p>\n<p>Дополнительная информация (необязательно)\n</p>\n<p>Для тех команд, которые разработали множество различных функций управления, они могут захотеть предоставить дополнительную информацию, чтобы помочь судьям в понимании их работы. Это место, где команды могут предоставить более подробную информацию о своих проектах. Она должна быть организована таким образом, чтобы отдельные темы были легко идентифицированы и их можно было быстро найти.</p>\n        </div>\n    );\n}\n\nexport default DocNineComponent;\n"]},"metadata":{},"sourceType":"module"}