\\u0493\\u0430 \\u049B\\u043E\\u0441\\u044B\\u043B\\u0443 \\u04AF\\u0448\\u0456\\u043D \\u049B\\u0430\\u0436\\u0435\\u0442. Control Hub \\u043F\\u0430\\u0439\\u0434\\u0430\\u043B\\u0430\\u043D\\u0443\\u0448\\u044B\\u043B\\u0430\\u0440\\u044B Expansion hub-\\u0442\\u0456 \\u049B\\u043E\\u0441\\u0443 \\u0430\\u0440\\u049B\\u044B\\u043B\\u044B \\u043F\\u0430\\u0439\\u0434\\u0430\\u043B\\u0430\\u043D\\u0430\\u0434\\u044B. REV Robotics \\u049B\\u043E\\u0441\\u049B\\u044B\\u0448\\u044B, \\u043A\\u0430\\u0431\\u0435\\u043B\\u044C \\u0436\\u0259\\u043D\\u0435 \\u043A\\u0440\\u043E\\u043D\\u0448\\u0442\\u0435\\u0439\\u043D (REV-31-1387).\"), /*#__PURE__*/React.createElement(\"p\", {\n    __self: this,\n    __source: {\n      fileName: _jsxFileName,\n      lineNumber: 119,\n      columnNumber: 3\n    }\n  }, \"\\u041C\\u0430\\u049B\\u04B1\\u043B\\u0434\\u0430\\u043D\\u0493\\u0430\\u043D 12 \\u0412 \\u0431\\u0430\\u0442\\u0430\\u0440\\u0435\\u044F\\u043D\\u044B \\u043F\\u0430\\u0439\\u0434\\u0430\\u043B\\u0430\\u043D\\u0441\\u0430\\u04A3\\u044B\\u0437 \\u043E\\u043D\\u044B\\u04A3 Tamiya \\u049B\\u043E\\u0441\\u049B\\u044B\\u0448\\u044B \\u0431\\u0430\\u0440 (\\u043C\\u044B\\u0441\\u0430\\u043B\\u044B Tetrix W39057 \\u0431\\u0430\\u0442\\u0430\\u0440\\u0435\\u044F\\u0441\\u044B) \\u0441\\u0456\\u0437\\u0433\\u0435 \\u049B\\u0430\\u0436\\u0435\\u0442 \\u0431\\u043E\\u043B\\u0430\\u0434\\u044B REV Robotics Tamiya - XT30 \\u0430\\u0434\\u0430\\u043F\\u0442\\u0435\\u0440\\u0456 \\u041A\\u0430\\u0431\\u0435\\u043B\\u044C (REV-31-1382). \\u0415\\u0433\\u0435\\u0440 \\u0441\\u0456\\u0437\\u0434\\u0435 REV Robotics Slim \\u0431\\u0430\\u0442\\u0430\\u0440\\u0435\\u044F\\u0441\\u044B \\u0431\\u043E\\u043B\\u0441\\u0430 (REV-31-1302) \\u043E\\u043D\\u0434\\u0430 \\u0441\\u0456\\u0437\\u0433\\u0435 \\u049B\\u0430\\u0436\\u0435\\u0442 \\u0431\\u043E\\u043B\\u043C\\u0430\\u0439\\u0434\\u044B \\u0431\\u04B1\\u043B \\u0430\\u0434\\u0430\\u043F\\u0442\\u0435\\u0440 REV \\u0431\\u0430\\u0442\\u0430\\u0440\\u0435\\u044F\\u0441\\u044B\\u043D\\u0430\\u043D \\u0431\\u0435\\u0440\\u0456 XT30 \\u049B\\u043E\\u0441\\u049B\\u044B\\u0448\\u044B \\u0259\\u043B\\u0434\\u0435\\u049B\\u0430\\u0448\\u0430\\u043D \\u0431\\u0430\\u0440.\"));\n}\n\nexport default softFiveComponent;","map":{"version":3,"sources":["/home/aruzhan/Desktop/project/src/routes/softFive/softFiveComponent.js"],"names":["React","Link","softFiveComponent"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,kCAAP;AACA,SAAQC,IAAR,QAAmB,kBAAnB;;AAGA,SAASC,iBAAT,GAA6B;AACzB,sBACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBAII;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDAJJ,eAOR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8sEAPQ,eAYR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4MAZQ,eAeR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,w6CAfQ,eAkBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yJAlBQ,eAqBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qiBArBQ,eAwBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,q6CAxBQ,eA2BR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,osDA3BQ,eA+BR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ynCA/BQ,eAoCR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,igBApCQ,eAwCR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yFAxCQ,eA2CR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,u2EA3CQ,eA+CR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iZA/CQ,eAkDR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,osCAlDQ,eAqDR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCArDQ,eAwDR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0nBAxDQ,eA4DR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6iCA5DQ,eAgER;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+KAhEQ,uyDAoEN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kqCApEM,eAuER;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yoCAvEQ,eA2EN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,o0CA3EM,eA8EN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2JA9EM,eAiFR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ylBAjFQ,eAoFN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8UApFM,eAuFN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,spBAvFM,eA+FN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4rDA/FM,eAyGN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2qCAzGM,eA+GN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4tCA/GM,CADJ;AA2HH;;AAED,eAAeA,iBAAf","sourcesContent":["import React from 'react';\nimport 'bootstrap/dist/css/bootstrap.css';\nimport {Link} from 'react-router-dom';\n\n\nfunction softFiveComponent() {\n    return (\n        <div>\n            \n         \n          \n            <p>\nКіріспе:\n</p>  \n<p>\n  FIRST Tech Challenge мақсаты-жастарды STEM көшбасшыларының келесі буыны болуға шабыттандыру жəне\nтəлімгердің басшылығымен робототехника конкурсына қатысу арқылы инноваторлар. FIRST Tech Challange-ке қатысатын командалар əртүрлі тапсырмаларды орындайтын робот жасау керек. Тапсырмалар маусымнан маусымға жəне\nəр маусымның басында жарияланатын ойын ережелерінің жиынтығына негізделген. Қаншалықты робот команда жасай алса, соншалықты ол көп ұпай жинай алады.\n</p>\n<p>\n  Автономды жəне жүргізуші басқаратын:\n</p>\n<p>\n  First Tech Challenge матчы автономды фазадан жəне жүргізуші басқаратын немесе теледидармен басқарылатын фазадан тұрады. Матчтың автономды кезеңінде робот адамның қатысуынсыз немесе бақылаусыз жұмыс істейді. Жүргізуші басқаратын кезеңде робот екі адам жүргізушісінен тапсырма ала алады.\n</p>\n<p>\n  \"Нүкте-нүкте\"басқару жүйесі:\n</p>\n<p>\n  The FIRST Tech Challenge роботтарын басқару үшін Android құрылғыларын пайдаланады. Жарыс кезінде əр командада екі Android device-тары болады.\n</p>\n<p>\nБір Android құрылғысы роботқа орнатылған жəне ол Robot Controller деп аталады. Robot Controller роботтың «миы» ретінде əрекет етеді. Ол роботтың барлық ойларын жасайды жəне роботқа не істеу керектігін айтады. Ол мынадан тұрады FTC Robot Controller қолданбасы бар Android құрылғысы. Аппараттық құралдың екі нұсқасы қазір қолданылуда: REV\n</p>\n<p>\nRobotics Expansion Hub немесе REV Robotics Control Hub.\nЕкінші Android құрылғысы команда драйверлерімен бірге отырады жəне бір немесе екі геймпад қосылған. Бұл екінші құрылғы Driver Station ретінде белгілі. Driver Station қашықтан басқару құралы сияқты, роботты басқару үшін пайдалануға болады. Driver Station командаға қашықтан (қауіпсіз, сымсыз қосылымды пайдалану арқылы) байланыс орнатуға мүмкіндік береді\n</p>\n<p>\nРобот контроллері жəне робот контроллеріне командалар беру. Driver Station Android құрылғысынан тұрады\n(смартфон немесе REV Driver Hub) FTC Driver Station қолданбасын іске қосады. REV драйвер хабында FTC драйвері бар\nСтанция қолданбасы құрылғыда алдын ала орнатылған. \n</p>\n<p>\n  Ескертпе: зарядталғаннан кейін қолданбаны хабқа қайта орнатудың қажеті жоқ\nWi-Fi желісіне қосылған..\n</p>\n<p>\n  REV Robotics кеңейту хабы:\n</p>\n<p>\nREV Robotics Expansion Hub робот контроллеріне мүмкіндік беретін электрондық кіріс/шығыс (немесе «I/O») модулі болып табылады роботтың қозғалтқыштарымен, серволарымен жəне сенсорларымен қосылады. Робот контроллері кеңейту хабымен байланысады сериялық қосылым арқылы. Android смартфоны Robot Controller ретінде пайдаланылатын жағдайда сериялық қосылымды орнату үшін USB кабелі қолданылады. REV Robotics Control Hub пайдаланылған жағдайда,\nкірістірілген Android құрылғысы мен кеңейту хабы арасында ішкі сериялық байланыс бар.\n</p>\n<p>\n  Expansion Hub 12 вольттық батареяға қосылған, сонымен сервер, моторларды, қуаттандырады.\n</p>\n<p>\n  Егер Android смартфоны робот контроллері ретінде пайдаланылса, смартфонда өзінің тəуелсіз батареясы болады. REV Robotics Control Hub робот контроллері ретінде пайдаланылса, басқару хабы ішкі Android құрылғысын қуаттандыру үшін негізгі 12 В батареяны пайдаланады.\n</p>\n<p>\n  REV Robotics Control Hub:\n</p>\n<p>\n  Controll Hub-та робот контроллерінің біріктірілген нұсқасы бар. Ол ішіне орнатылған Android құрылғысын біріктіреді\nREV Robotics Expansion Hub сияқты корпус.\n</p>\n<p>\n  Controll hub, оның кірістірілген Android құрылғысы ішкі арқылы кеңейту хабына тікелей қосылған\nсериялық автобус Android робот контроллері мен енгізу/шығару арасындағы сыртқы USB қосылымының қажеттілігін болдырмайтын I/O модуль\n</p>\n<p>\n  Операциялық режим дегеніміз не?:\n</p>\n  Əдеттегі FIRST Tech Challenge матчында команданың роботы əртүрлі тапсырмаларды орындауда ұпай жинайды. Мысалы, команда өз роботының жарыс алаңындағы ақ сызықты, содан кейін жүруін қалауы мүмкін матч кезінде ойын элементін (мысалы, допты) өз бетінше қақпаға соғады. Командалар «іс режимдерін» жазады (ол роботының əрекетін көрсету үшін «жұмыс режимдерін» білдіреді).\n  <p>\nОперациялық режимдер – бəсекелес роботтың əрекетін реттеу үшін пайдаланылатын компьютерлік бағдарламалар. Робот Контроллер сəйкестік кезінде белгілі бір тапсырмаларды орындау үшін таңдалған жұмыс режимін орындай алады.\n</p>\n<p>\nFIRST Tech Challenge-ге қатысатын командаларда олар пайдалана алатын түрлі бағдарламалау құралдары бар.\nөздерінің жұмыс режимдерін жасау. Командалар FTC блоктары деп аталатын көрнекі («сүйреп апару») бағдарламалау құралын пайдалана алады\n    </p>\n  <p>\nОлардың жұмыс режимдерін жасау үшін бағдарламалау құралы. Командалар FTC OnBot деп аталатын мəтіндік Java құралын да пайдалана алады Java бағдарламалау құралы немесе Google компаниясының Android Studio біріктірілген əзірлеу ортасы (сонымен бірге «IDE») олардың жұмыс режимдерін жасау үшін.\n</p>\n  <p>\n    Қажетті материалдар тізімі:\n  </p>\n<p>\n  Бұл нұсқаулықта FTC басқару жүйесін конфигурациялау, бағдарламалау жəне пайдалану жолын көрсететін оқулықтар бар.\n</p>\n  <p>\n    Оқулықтарды аяқтау үшін сізде келесі материалдар болуы керек:\n  </p>\n  <p>\n  Екі (2) FIRST мақұлдаған* Android\n  құрылғылар (Robot Controller смартфоны\nжəне Driver Station смартфоны немесе\nDriver Hub).\nНемесе...\nБір (1) Controll хабы жəне бір (1) FIRST мақұлдаған* Android смартфоны.\n  </p>\n  <p>\n    Microsoft Windows 7, 8 немесе\n10 жəне Wi-Fi мүмкіндігі.\nЕскерту: ноутбукта ең ағымдағы қызмет пакеттері мен жүйесі Microsoft жаңартулары болуы керек.\n\n Басқа түрін пайдалансаңыз\nқұрылғы (мысалы, Chromebook,\nAndroid планшеті жəне т.б.) сіздің ретінде бағдарламалау құрылғысы, қадамдар болуы мүмкін қол жеткізу жолында аздап ерекшеленеді\nJavascript қосылған веб-шолғыш (Google Chrome — ұсынылатын шолғыш).\n   </p>\n  <p>\n    Егер сіз смартфонды Робот контроллері ретінде пайдалансаңыз, сізге REV Robotics Expansion hub (REV-31-\n1153) қозғалтқыштарға, серволарға,\nжəне сенсорларға қосылу үшін қажет. Control Hub пайдаланушылары Expansion hub-ті қосу арқылы пайдаланады.\nREV Robotics қосқышы, кабель жəне кронштейн (REV-31-1387).\n  </p>\n  <p>\n    Мақұлданған 12 В батареяны пайдалансаңыз оның Tamiya қосқышы бар (мысалы\nTetrix W39057 батареясы) сізге қажет болады REV Robotics Tamiya - XT30 адаптері Кабель (REV-31-1382).\nЕгер сізде REV Robotics Slim батареясы болса (REV-31-1302) онда сізге қажет болмайды\nбұл адаптер REV батареясынан бері\nXT30 қосқышы əлдеқашан бар.\n  </p>\n        \n        </div>\n\n    );\n}\n\nexport default softFiveComponent;\n"]},"metadata":{},"sourceType":"module"}